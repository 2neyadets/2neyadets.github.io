.managerWB__card-dialog
  z-index 6001
  .q-dialog__inner
    align-items flex-end !important
  .q-dialog__backdrop
    opacity 0
    pointer-events none
  .q-layout-container > div > div
    max-height none

.managerWB__stat-dialog
  z-index 7001

.managerWB__project, .managerWB__dialog
  line-height 1.5
  font-family 'HelveticaNeueCyr', 'Helvetica Neue', Helvetica, Arial, sans-serif
  overflow hidden
  color black

  ::-webkit-scrollbar
    width 8px
    height 8px

  ::-webkit-scrollbar-track
    background transparent

  ::-webkit-scrollbar-thumb
    background black

  p
    text-indent 0 !important

  .q-dark
    color white
    background #313131 !important

  .body--dark &
    color white
    background #313131 !important

  .desktop &
    width 100vw
    height calc(100vh - 32px)

  .managerWB-page
    min-width 100vw
    min-height calc(100vh - 32px)
    overflow hidden

  $WB-filters-border-color ?= rgba(255, 255, 255, 0.28)

  .filters
    font-size $font-size-medium
    border 1px solid
    border-radius 4px
    border-color $WB-filters-border-color

    input[type=number]::-webkit-inner-spin-button,
    input[type=number]::-webkit-outer-spin-button
      -webkit-appearance none
      margin 0

    .filter__block
      border 1px solid
      border-radius 4px
      border-color $WB-filters-border-color
      background $WB-primary-color
      min-height 75px

    .q-field
      &__native
        overflow-y auto
        color $WB-bg-profile-color

      &__control-container
        padding 1px 0 2px
        //max-height calc(100% - 2px)

        .q-field__native
          min-height 100%
          padding 4px 0

      &--with-bottom
        padding 0 0

      &--dark, &--readonly
        .q-field__control:before
          transition border-color 0.36s cubic-bezier(0.4, 0, 0.2, 1)

      .q-field__control:hover:before
        border-color #000

      &--dark
        .q-field__control:before
          border 1px solid rgba(0,0,0,0.24)
      &--readonly
        .q-field__control:before
          border 1px dashed rgba(0,0,0,0.24)

  .my-sticky-header-table

    &-hard

      .q-table th, .q-table td
        padding 7px 4px

      thead th, tbody td
        width 7.5%

        &:first-child
          width 25%

    &-soft

      thead th, tbody td
        width 12%

        &:first-child
          width 28%

    &-hard, &-soft
      min-width 1285px

      .bottom-row
        padding-right 8px

      .q-table__middle
        max-height 83vh

      .q-table__top, .q-table__bottom, thead tr th
        background-color #616161 !important

      .q-table__bottom
        padding 0

      thead tr th
        position sticky
        z-index 1

      thead tr:first-child th
        top 0

      thead tr:last-child th
        top 51px


      &.q-table--loading thead tr:last-child th
        top 51px

  .schedule__block
    border 1px solid
    border-radius 5px
    border-color $WB-border-comments-color
    background $WB-primary-color

  .btn-for-schedule
    height 21px
    width 21px

    .q-btn__wrapper
      padding 0

  .schedule__table
    border 1px solid
    border-radius 5px
    border-color $WB-border-comments-color

  .schedule__choose-timesheet
    border 1px solid
    border-radius 5px
    border-color $WB-border-comments-color

  .border-top
    border-top 1px solid
    border-color $WB-border-comments-color

  .border-left
    border-left 1px solid
    border-color $WB-border-comments-color

  .border-right
    border-right 1px solid
    border-color $WB-border-comments-color

  .border-bottom
    border-bottom 1px solid
    border-color $WB-border-comments-color

  .employee-dialog__card .q-field__native
    color $WB-bg-profile-color

  .status-border
    border 1px solid
    border-radius 5px
    border-color $WB-border-comments-color

  .employee-dialog__card .q-field--with-bottom
    padding 0 0

  .q-select
    .q-chip
      background $grey-4 !important


